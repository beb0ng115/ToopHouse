<div class="q_slider">
    <div class="q_slider_inner">

        <div id="qode-home-page" class="carousel slide  q_auto_start   header_not_transparent" data-slide_animation="6000" style="height: 590px;">
            <div class="qode_slider_preloader" style="height: 591px; display: none;">
                <div class="ajax_loader_slider">
                    <div class="ajax_loader_1">
                        <div class="ajax_loader_html"></div>
                    </div>
                </div>
            </div>
            <div class="carousel-inner skrollable skrollable-between" data-start="transform: translateY(0px);" data-1440="transform: translateY(-500px);" style="transform: translateY(0px);">
                <div class="item  active" style="height: 590px;">
                    <div class="video">
                        <div class="mobile-video-image" style="background-image: url(~/Content/Images/for_web-slow-preview.jpg)"></div>
                        <div class="video-overlay">
                            <img src="~/Content/Images/pixel-video.png" alt="">
                        </div>
                        <div class="video-wrap" style="width: 1349px; height: 591px;">
                            <div id="mep_0" class="mejs-container svg video mejs-video" style="width: 1920px; height: 800px;">
                                <div class="mejs-inner">
                                    <div class="mejs-mediaelement">
                                        <video class="video" width="1920" height="800" poster="~/Content/Images/for_web-slow-preview.jpg" preload="auto" loop="" autoplay="" muted=""
                                               src="~/Content/Videos/for_web-slow.mp4" style="width: 1351px; height: 761px;">
                                            <source type="video/mp4" src="~/Content/Videos/for_web-slow.mp4">
                                        </video>
                                    </div>
                                    <div class="mejs-controls" style="visibility: hidden; display: block;">
                                        <div class="mejs-button mejs-playpause-button mejs-pause">
                                            <button type="button" aria-controls="mep_0" title="Play/Pause" aria-label="Play/Pause"></button>
                                        </div>
                                        <div class="mejs-time mejs-currenttime-container"><span class="mejs-currenttime">00:03</span></div>
                                        <div class="mejs-time-rail" style="width: 99px;">
                                            <span class="mejs-time-total" style="width: 69px;">
                                                <span class="mejs-time-buffering" style="display: none;"></span>
                                                <span class="mejs-time-loaded" style="width: 69px;"></span>
                                                <span class="mejs-time-current" style="width: 37px;"></span>
                                                <span class="mejs-time-handle" style="left: 32px;"></span>
                                                <span class="mejs-time-float">
                                                    <span class="mejs-time-float-current">00:00</span>
                                                    <span class="mejs-time-float-corner"></span>
                                                </span>
                                            </span>
                                        </div>
                                        <div class="mejs-time mejs-duration-container"><span class="mejs-duration">00:06</span></div>
                                        <div class="mejs-button mejs-volume-button mejs-mute">
                                            <button type="button" aria-controls="mep_0" title="Mute Toggle" aria-label="Mute Toggle"></button>
                                            <div class="mejs-volume-slider">
                                                <div class="mejs-volume-total"></div>
                                                <div class="mejs-volume-current"></div>
                                                <div class="mejs-volume-handle"></div>
                                            </div>
                                        </div>
                                        <div class="mejs-button mejs-fullscreen-button">
                                            <button type="button" aria-controls="mep_0" title="Fullscreen" aria-label="Fullscreen"></button>
                                        </div>
                                    </div>
                                    <div class="mejs-clear"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="slider_content_outer">
                        <div class="slider_content right skrollable skrollable-between" style="width: 40%; right: 0%; top: 30%; opacity: 1;" data-start="width:40%; opacity:1; right:0%; top:30%;" data-300="opacity: 0; right:0%; top:20%;">
                            <div class="thumb flip">
                                <a href=@Url.Action("Index", "Renting")>
                                    <img src="~/Content/Images/homepage-slider-deal.png" alt="Affordable Photo Studio<br> for All Your Needs">
                                </a>
                            </div>
                        </div>
                        <div class="slider_content left skrollable skrollable-between" style="margin-left: 20px; width: 60%; left: 0%; top: 40%; opacity: 1;" data-start="width:60%; opacity:1; left:0%; top:40%;" data-300="opacity: 0; left:0%; top:30%;">
                            <div class="text">
                                <h2 style="font-weight: 800;"><span>ToopHouse Photo Studio<br> for All Your Needs</span></h2>
                                <p style=""><span>Duy nhất tại Vũng Tàu</span></p>
                                <a class="qbutton small dark" href=@Url.Action("Calendar", "Renting")>Đặt Lịch</a>
                                <a class="qbutton small green" href=@Url.Action("Calendar", "Renting")>Đánh giá</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>